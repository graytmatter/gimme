HAMBERGER

<img id="icon" src="<%= @pic %>">


<%= form_for @wishlist do |f| %>

	<%= f.label :title %>:
  	<%= f.text_field :title %><br />

	<%= f.label :date %>:
	<%= f.text_field :date, id: "datepicker"%><br />

	<%= f.label :description %>:
	<%= f.text_field :description %><br />

	<%= f.label :type %>:<br>
  	<%= f.radio_button(:kind, "christmas") %> Christmas <br />
  	<%= f.radio_button(:kind, "birthday") %> Birthday <br />
  	<%= f.radio_button(:kind, "other") %> Other <br />

  <%= f.submit %>
<% end %>