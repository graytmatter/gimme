<h1>All Wishlists</h1>

<% @wishlists.each do |wish| %>
	<div class='wishlist'>
	<a href="/wishlists/<%= wish.id %>">
  <p><%= wish.title %></p>
  </a>
  <br>

</div>
<% end %>


